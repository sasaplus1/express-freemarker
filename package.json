{
  "name": "express-freemarker",
  "description": "FreeMarker template engine for Express",
  "version": "1.0.0",
  "author": "sasa+1 <sasaplus1@gmail.com>",
  "dependencies": {
    "freemarker.js": "^1.4.0"
  },
  "devDependencies": {
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-node": "^11.0.0",
    "express": "^4.17.1",
    "husky": "^4.2.1",
    "intelli-espower-loader": "^1.0.1",
    "lint-staged": "^10.0.7",
    "mocha": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "power-assert": "^1.6.1",
    "prettier": "^2.0.0"
  },
  "files": [
    "index.js"
  ],
  "license": "MIT",
  "main": "./index.js",
  "private": true,
  "readmeFilename": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/sasaplus1/express-freemarker.git"
  },
  "scripts": {
    "fix": "run-s fix:eslint fix:prettier",
    "fix:eslint": "eslint --fix .",
    "fix:prettier": "prettier --write .",
    "fixpack": "npx fixpack",
    "lint": "run-s lint:eslint lint:prettier",
    "lint:eslint": "eslint --cache .",
    "lint:prettier": "prettier --check .",
    "mocha": "mocha",
    "pretest": "DOCKER_BUILDKIT=1 docker build --tag sasaplus1/express-freemarker .",
    "sh": "docker run --rm -it -v \"$PWD:/opt/express-freemarker\" sasaplus1/express-freemarker bash",
    "test": "docker run --rm -t sasaplus1/express-freemarker yarn run mocha"
  }
}
